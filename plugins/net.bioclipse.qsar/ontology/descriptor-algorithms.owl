    </definition>
    <isClassifiedAs rdf:resource="electronicDescriptor"/>
    <isClassifiedAs rdf:resource="geometricalDescriptor"/>
    <isClassifiedAs rdf:resource="molecularDescriptor"/>
</Descriptor>


