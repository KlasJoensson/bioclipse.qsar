<!-- classes and properties -->

<owl:Class rdf:ID="Category">
  <rdfs:label>Category</rdfs:label>
</owl:Class>

<owl:Class rdf:ID="Algorithm">
  <rdfs:label>Algorithm</rdfs:label>
</owl:Class>

<owl:Class rdf:ID="Descriptor">
  <rdfs:label>Descriptor</rdfs:label>
  <owl:subClassOf rdf:resource="&me;Algorithm"/>
  <rdfs:comment>
    A feature (descriptor) is an algorithm that operates on a chemical
    entity (commonly atoms and molecules)
    and returns a numerical or nominal (descriptor) value which described
    certain information on that entity.
  </rdfs:comment>
</owl:Class>

<owl:Class rdf:ID="Contributor">
  <rdfs:label>Contributor</rdfs:label>
</owl:Class>

<owl:Class rdf:ID="DataFeature">
  <rdfs:label>Data Feature</rdfs:label>
</owl:Class>

<owl:Class rdf:ID="DataRepresentation">
  <rdfs:label>Data Representation</rdfs:label>
  <owl:subClassOf rdf:resource="&me;DataFeature"/>
</owl:Class>

<owl:Class rdf:ID="Implementation">
  <rdfs:label>Implementation</rdfs:label>
</owl:Class>

<owl:ObjectProperty rdf:ID="isA">
  <rdfs:label>Is A</rdfs:label>
  <rdfs:domain rdf:resource="&owl;Thing"/>
  <rdfs:range rdf:resource="&owl;Thing"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="instanceOf">
  <rdfs:label>Instance Of</rdfs:label>
  <rdfs:domain rdf:resource="&owl;Implementation"/>
  <rdfs:range rdf:resource="&owl;Algorithm"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="isClassifiedAs">
  <rdfs:label>Is Classified As</rdfs:label>
  <rdfs:domain rdf:resource="&owl;Thing"/>
  <rdfs:range rdf:resource="&me;Category"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:ID="requires">
  <rdfs:label>Requires</rdfs:label>
  <rdfs:domain rdf:resource="&me;Algorithm"/>
  <rdfs:range rdf:resource="&me;DataFeature"/>
</owl:ObjectProperty>

<owl:DataProperty rdf:ID="definition">
  <rdfs:domain rdf:resource="&owl;Thing"/>
  <rdfs:range rdf:resource="&rdfs;XMLLiteral"/>
</owl:DataProperty>

<owl:DataProperty rdf:ID="description">
  <rdfs:domain rdf:resource="&owl;Thing"/>
  <rdfs:range rdf:resource="&rdfs;XMLLiteral"/>
</owl:DataProperty>

<!-- the facts: Data Features -->

<DataFeature rdf:ID="has2DCoordinates">
  <rdfs:label>has 2D Coordinates</rdfs:label>
</DataFeature>

<DataFeature rdf:ID="has3DCoordinates">
  <rdfs:label>has 3D Coordinates</rdfs:label>
</DataFeature>

<DataFeature rdf:ID="hasFractionalUnitCellCoordinatesCoordinates">
  <rdfs:label>has Fractional Unit Cell Coordinates Coordinates</rdfs:label>
  <isClassifiedAs rdf:resource="&me;crystallography"/>
</DataFeature>

<DataFeature rdf:ID="hasPartialAtomicCharges">
  <rdfs:label>has Partial Atomic Charges</rdfs:label>
</DataFeature>

<DataRepresentation rdf:ID="DietzRepresentation">
  <rdfs:label>Dietz Representation</rdfs:label>
  <owl:complementOf rdf:resource="&me;GraphRepresentation"/>
  <!-- FIXME: add description and bibtex:article to Dietz' article in JCICS -->
</DataRepresentation>

<DataRepresentation rdf:ID="GraphRepresentation">
  <rdfs:label>Graph Representation</rdfs:label>
  <owl:complementOf rdf:resource="&me;DietzRepresentation"/>
  <description>
    Representation in which atoms are represented by graph nodes and 
    bonds by graph edges. Consequently, bonds are always localized
    between two atoms.
  </description>
</DataRepresentation>

