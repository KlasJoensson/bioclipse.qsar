<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="net.bioclipse.scripting.contribution">
      <scriptContribution
            id="net.bioclipse.cdk.business.CDKManager"
            service="net.bioclipse.cdk.business.CDKManagerFactory">
      </scriptContribution>
   </extension>
   <extension
         point="org.eclipse.core.runtime.adapters">
      <factory
            adaptableType="org.eclipse.core.resources.IFile"
            class="net.bioclipse.cdk.domain.CDKAdapterFactory">
         <adapter
               type="net.bioclipse.cdk.domain.ICDKMolecule">
         </adapter>
      </factory>
   </extension>

<extension
      point="net.bioclipse.qsar.descriptorProvider">
      <category
            id="net.bioclipse.qsar.cdk.molecular"
            name="Molecular"
            icon="icons/molecule2D.png">
      </category>
         <provider
            id="net.bioclipse.cdk.descriptorprovider"
            name="Chemistry Development Kit (CDK)"
            icon="icons/molecule2D.png"
            calculator="net.bioclipse.cdk.qsar.impl.CDKDescriptorCalculator"
            acceptsSmiles="true"
            acceptsCml="true"
            acceptsMolfile="true">
            <descriptor
                  category="net.bioclipse.qsar.cdk.molecular"
                  id="org.openscience.cdk.qsar.descriptors.molecular.XLogPDescriptor"
                  name="XLogP">
                  <parameter
                        defaultvalue="true"
                        key="checkAromaticity">
                  </parameter>
                  <parameter
                        defaultvalue="true"
                        key="salicylFlag">
                  </parameter>
            </descriptor>
            <descriptor
                  category="net.bioclipse.qsar.cdk.molecular"
                  id="org.openscience.cdk.qsar.descriptors.molecular.BPolDescriptor"
                  name="BPol">
            </descriptor>
         </provider>
</extension>

</plugin>
