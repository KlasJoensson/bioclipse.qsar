<plugin>

   <extension point="org.eclipse.ui.newWizards">
      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="net.bioclipse.qsar.presentation.QsarModelWizardID"
            name="%_UI_QsarModelWizard_label"
            class="net.bioclipse.qsar.presentation.QsarModelWizard"
            category="org.eclipse.emf.ecore.Wizard.category.ID"
            icon="icons/full/obj16/QsarModelFile.gif">
         <description>%_UI_QsarModelWizard_description</description>
         <selection class="org.eclipse.core.resources.IResource"/>
      </wizard>
   </extension>

   <extension point="org.eclipse.ui.editors">
      <editor
            id="net.bioclipse.qsar.presentation.QsarEditorID"
            name="%_UI_QsarEditor_label"
            icon="icons/full/obj16/QsarModelFile.gif"
            extensions="qsar"
            class="net.bioclipse.qsar.presentation.QsarEditor"
            contributorClass="net.bioclipse.qsar.presentation.QsarActionBarContributor">
      </editor>
   </extension>

</plugin>
